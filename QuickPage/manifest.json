{
	"manifest_version": 2,
    "name": "Quickpage",
    "short_name": "Quickpage",
    "description": "Customize your New Tab page with unique and useful widgets",
    "version": "3.00",
    "key": "",
    "browser_action": {
    	"default_popup": "checklist/index.html",
    	"default_icon" : "res/icon_tab.png"
    },
    "icons": {
    	"64": "res/icon_64.png",
    	"128": "res/icon_128.png",
    	"256": "res/icon_256.png"
    },
	 "options_ui": {
		 "page": "options/options.html",
		 "open_in_tab": false
	 },
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "permissions" : [
        "management",
        "identity",
        "storage",
        "geolocation",
        "file:///*",
		  "activeTab"
    ]
}
